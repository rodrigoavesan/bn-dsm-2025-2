generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// Nomes de model no Prisma devem estar
// no singular e iniciar com LetraMai√∫scula
model Categoria {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  descricao   String
}